# Variables de entorno para la aplicación

# Puerto del servidor
PORT=3000

# Configuración de la base de datos
# En entorno de desarrollo local
DB_HOST=localhost
DB_USER=root
DB_PASSWORD=tu_contraseña
DB_NAME=registro_pasajeros

# En Render, estas variables se configuran automáticamente desde render.yaml
# No es necesario configurarlas manualmente en el entorno de producción

# Configuración del entorno
# Valores posibles: development, production
NODE_ENV=development

# Entorno de ejecución (development, production)
NODE_ENV=development